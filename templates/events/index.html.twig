{% extends 'base.html.twig' %}


{% block page_contents %}

    <table class="table">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
        </tr>
        </thead>
        <tbody>
        {% for event in events %}
            <tr>
                <th scope="row">{{ event.id }}</th>
                <td>{{ event.name }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>


{% endblock %}
